@model all_about
@{
    Layout = "~/Views/Shared/admin_layout.cshtml";
}



<section class="ls section_padding_top_50 section_padding_bottom_50 columns_padding_10">
    <div class="container-fluid">
        <section class="ls section_padding_top_50 section_padding_bottom_50 columns_padding_10">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-12">
                        <h3>ABOUT US</h3>
                    </div>
                </div>
                <!-- .row -->

                <div class="row justify-content">
                    <!-- Center the form -->
                    <div class="col-xs-12 col-md-6">
                        <!-- Basic form -->
                        <div class="form-container">
                            <!-- Added a container for the form -->
                            <form method="post" enctype="multipart/form-data" >
                                <!-- Form elements go here -->

                                <h4>
                                    <b><center>WHAT WE DO</center></b>
                                </h4>
                                <br />
                                <div class="form-group">
                                    <label for="description"><h5>Description:</h5></label>
                                    <input type="text" asp-for="about_wwd.desc"
                                           class="form-control"
                                           placeholder="Enter Description" />
                                </div>

                                <div class="form-group">
                                    <label for="picture"><h5>Add Image:</h5></label>
                                    <input type="file"
                                          name="img11"
                                           class="form-control"
                                </div>
                                <br />
                                <button type="submit" class="theme_button" asp-action="whatwedo" asp-controller="Admin">ADD</button>
                            </form>
                            <p>@ViewBag.e</p>
                        </div>
                    </div>
                    <!-- .col-* -->
                </div>

                <br />

                <div class="row justify-content">
                    <!-- Center the form -->
                    <div class="col-xs-12 col-md-6">
                        <!-- Basic form -->
                        <div class="form-container">
                            <!-- Added a container for the form -->
                            <form method="post" enctype="multipart/form-data">
                                <!-- Form elements go here -->

                                <h4>
                                    <b><center>Our Mission</center></b>
                                </h4>
                                <br />
                                <div class="form-group">
                                    <label for="description"><h5>Description:</h5></label>
                                    <input type="text" 
                                        asp-for="about_Om.desc"
                                           class="form-control"
                                           placeholder="Enter Description" />
                                </div>

                                <div class="form-group">
                                    <label for="picture"><h5>Add Image:</h5></label>
                                    <input type="file"
                                           name="img"
                                           class="form-control"
                                           placeholder="Enter Picture URL" />
                                  
                                </div>
                                <br />
                                <button type="submit" class="theme_button" asp-action="Ourmission" asp-controller="admin">ADD</button>
                            </form>
                        </div>
                    </div>
                    <!-- .col-* -->
                </div>
                <br>
                <div class="row justify-content">
                    <!-- Center the form -->
                    <div class="col-xs-12 col-md-6">
                        <!-- Basic form -->
                        <div class="form-container">
                            <!-- Added a container for the form -->
                            <form method="post" enctype="multipart/form-data">
                                <!-- Form elements go here -->

                                <h4>
                                    <b><center>OUR TEAM</center></b>
                                </h4>
                                <br />
                                <div class="form-group">
                                    <label ><h5>Name:</h5></label>
                                    <input type="text"
                                       asp-for="about_team.name"
                                           class="form-control"
                                           placeholder="Enter Name"
                                           required />
                                </div>

                                <div class="form-group">
                                    <label for="name"><h5>Designation:</h5></label>
                                    <input type="text" asp-for="about_team.designation"
                                           class="form-control"
                                           placeholder="Enter Designation"
                                           required />
                                </div>

                                <div class="form-group">
                                    <label for="picture"><h5>Add Image:</h5></label>
                                    <input type="file"
                                          name="img"
                                           class="form-control"
                                           placeholder="Insert Image" />
                                   
                                </div>
                                <br />
                                <button type="submit" class="theme_button" asp-action="ourteam" asp-controller="admin">ADD</button>
                            </form>
                        </div>
                    </div>
                    <!-- .col-* -->
                </div>

                  <br />

                <div class="row justify-content">
                    <!-- Center the form -->
                    <div class="col-xs-12 col-md-6">
                        <!-- Basic form -->
                        <div class="form-container">
                            <!-- Added a container for the form -->
                            <form method="post" enctype="multipart/form-data">
                                <!-- Form elements go here -->

                                <h4>
                                    <b><center>Career with us</center></b>
                                </h4>
                                <br />
                                <div class="form-group">
                                    <label for="name"><h5>Name:</h5></label>
                                    <input type="text"
                                         asp-for="about_Cwu.name"
                                           class="form-control"
                                           placeholder="Enter Name"
                                           required />
                                </div>

                                <div class="form-group">
                                    <label for="email"><h5>Email:</h5></label>
                                    <input type="email"
                                          asp-for="about_Cwu.email"
                                           class="form-control"
                                           placeholder="Enter Email"
                                           required />
                                </div>

                                <div class="form-group">
                                    <label for="contact"><h5>Contact:</h5></label>
                                    <input type="text"
                                          asp-for="about_Cwu.contact"
                                           class="form-control"
                                           placeholder="Enter Contact Number"
                                           required />
                                </div>

                                <div class="form-group">
                                    <label for="cv"><h5>CV:</h5></label>
                                    <input type="file"
                                        name="file"
                                           class="form-control"
                                           placeholder="Upload CV"
                                           accept=".pdf,.doc,.docx"
                                           required />
                                </div>
                                <br />
                                <button type="submit" asp-action="Careerwithus" asp-controller="admin" class="theme_button">ADD</button>
                            </form>
                        </div>
                    </div>
                    <!-- .col-* -->
                </div>
               <br />
                <div class="row justify-content">
                    <!-- Center the form -->
                    <div class="col-xs-12 col-md-6">
                        <!-- Basic form -->
                        <div class="form-container">
                            <!-- Added a container for the form -->
                            <form method="post" enctype="multipart/form-data">
                                <!-- Form elements go here -->

                                <h4>
                                    <b><center>OUR SUPPORTERS</center></b>
                                </h4>
                                <br />

                                <div class="form-group">
                                    <label for="picture"><h5>Insert Image of first Supporter</h5></label>
                                    <input type="file"
                                           name="imga"
                                           class="form-control"
                                           placeholder="Insert Image of first Supporter " />
                                </div>
                                <div class="form-group">
                                    <label for="picture"><h5>Insert Image of Second Supporter</h5></label>
                                    <input type="file"
                                          name="imgb"
                                           class="form-control"
                                           placeholder="Insert Image of Second Supporter " />
                                </div>
                                <div class="form-group">
                                    <label for="picture"><h5>Insert Image of third Supporter</h5></label>
                                    <input type="file"
                                           name="imgc"
                                           class="form-control"
                                           placeholder="Insert Image of third Supporter " />
                                </div>
                                <div class="form-group">
                                    <label for="picture"><h5>Insert Image of fourth Supporter</h5></label>
                                    <input type="file"
                                           name="imgd"
                                           class="form-control"
                                           placeholder="Insert Image of fourth Supporter " />
                                </div>
                                <br />
                                <button type="submit"asp-action="about_os" asp-controller="admin" class="theme_button">ADD</button>
                            </form>
                        </div>
                    </div>
                    <!-- .col-* -->
                </div>

                <!-- .row -->
            </div>
            <!-- .container-fluid -->
        </section>
    </div>
</section>

