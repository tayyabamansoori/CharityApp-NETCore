@model IEnumerable<about_team>
@{
    ViewData["Title"] = "Partners";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="page_breadcrumbs offset_breadcrumbs ds ms parallax section_padding_50">
	<div class="container with_top_border">
		<div class="row">
			<div class="col-sm-6 text-center text-sm-left">
				<h2>Partners</h2>
			</div>
			<div class="col-sm-6 text-center text-sm-right">
				<ol class="breadcrumb darklinks grey">
					<li>
						<a asp-action="index" asp-controller="home">
							Home
						</a>
					</li>
					<li>
						<a href="#">Pages</a>
					</li>
					<li class="active">Partners</li>
				</ol>
			</div>
		</div>
	</div>
</section>
<section id="volunteers" class="ls team_section parallax section_padding_100">
	<div class="container">
		<div class="row">
			<div class="col-md-10 col-md-offset-1 text-center">
				<h2 class="section_header bold">
					Our
					<span class="highlight">Partners</span>
				</h2>
				<p>
					Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum.
				</p>
				<p class="topmargin_30">
					<a asp-action="careers" asp-controller="home" class="theme_button margin_0">Become a Partners</a>
				</p>
			</div>
		</div>
		 @foreach (var i in Model)
                {
		<div class="row topmargin_60">
			<div class="col-sm-12">
				<div class="owl-carousel" data-responsive-lg="3" data-nav="false" data-dots="false">
					<div class="vertical-item team-item text-center">
						<div class="item-media">
							  <img src="~/@i.img" />
						</div>
						<div class="item-content">
							<h3 class="entry-title">
									@i.name
								
							</h3>
							<p>@i.designation</p>
						
						</div>
					</div>

				
				</div>
			</div>
		</div>
		}
	</div>
</section>

					