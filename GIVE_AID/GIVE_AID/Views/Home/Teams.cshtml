@model IEnumerable<about_team>
@{
    ViewData["Title"] = "Teams";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="page_breadcrumbs offset_breadcrumbs ds ms parallax section_padding_50">
	<div class="container with_top_border">
		<div class="row">
			<div class="col-sm-6 text-center text-sm-left">
				<h2>Team</h2>
			</div>
			<div class="col-sm-6 text-center text-sm-right">
				<ol class="breadcrumb darklinks grey">
					<li>
						<a asp-action="home" asp-controller="index">
							Home
						</a>
					</li>
					<li>
						<a href="#">Pages</a>
					</li>
					<li class="active">Team</li>
				</ol>
			</div>
		</div>
	</div>
</section>


	<section class="ls section_padding_top_100 section_padding_bottom_50">
		<div class="container">
			<div class="row bottommargin_60">
				<div class="col-md-10 col-md-offset-1 text-center">
					<h2 class="section_header bold">
						Our
						<span class="highlight">Team</span>
					</h2>
					<p>
						Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum.
					</p>
				<p class="topmargin_30">
					<a asp-action="careers" asp-controller="home" class="theme_button margin_0">Become a volunteer</a>
				</p>
				</div>
			</div>
			<div class="isotope_container isotope row masonry-layout columns_margin_bottom_60" data-filters=".isotope_filters">
	@foreach (var i in Model)
		{
				<div class="isotope-item col-lg-4 col-md-6 col-sm-12">
		<div class="vertical-item team-item text-center">
			<div class="item-media">

								<img src="~/@i.img" /> 
			</div>
			<div class="item-content">
				<h3 class="entry-title">
					@i.name
					
				</h3>
				<p>@i.designation</p>
								
			</div>
		</div>
	</div>
	}
</section>

